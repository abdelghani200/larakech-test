{
    "version": 2,
    "builds": [
        {
            "src": "build.sh",
            "use": "@vercel/static-build"
        },
        {
            "src": "api/index.php",
            "use": "@vercel/php"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "outputDirectory": "public"
}
